        "yii2-videojs-widget",
        "yii2-videojs",
        "videojs",
        "html5 video"
    ],
    "type": "yii2-extension",
    "license": "BSD-3-Clause",
    "support": {
        "issues": "https://github.com/xutl/yii2-videojs-widget/issues",
        "wiki": "https://github.com/xutl/yii2-videojs-widget/wiki/",
        "source": "https://github.com/xutl/yii2-videojs-widget"
    },
    "require": {
        "yiisoft/yii2": "~2.0.0"
    },
    "autoload": {
        "psr-4": {
            "xutl\\alipay\\": ""
        }
    }
}
